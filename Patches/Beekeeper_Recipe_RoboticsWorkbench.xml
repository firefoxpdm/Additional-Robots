						<RecipeDef ParentName="RPP_Recipe_Bot_IV">
							<defName>RPP_Production_Beekeeper_IV</defName>
							<label>make planter bot (Tier 4)</label>
							<description>Produce a unit capable of beekeeping with system-master-class ability.</description>
							<products>
								<RPP_RechargeStation_Beekeeper_IV>1</RPP_RechargeStation_Beekeeper_IV>
							</products>
							<researchPrerequisite>RPP_Research_Beekeeper_All</researchPrerequisite>
						</RecipeDef>
						<RecipeDef ParentName="RPP_Recipe_Bot_V">
							<defName>RPP_Production_Beekeeper_V</defName>
							<label>make planter bot (Tier 5)</label>
							<description>Produce a unit capable of beekeeping with system-master-class ability.</description>
							<products>
								<RPP_RechargeStation_Beekeeper_V>1</RPP_RechargeStation_Beekeeper_V>
							</products>
							<researchPrerequisite>RPP_Research_Beekeeper_All</researchPrerequisite>
						</RecipeDef>

					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
